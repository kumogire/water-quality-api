APP_NAME="USGS Water Quality Reporting API"
ENV=development
DEBUG=true
LOG_LEVEL=DEBUG

# API Configuration
CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Monitoring (Optional)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project
ENABLE_METRICS=true

# USGS API Configuration
MAX_RECORDS_PER_REQUEST=10000
REQUEST_TIMEOUT=30